{
  "name": "json-rules-engine-simplified",
  "version": "0.1.18",
  "description": "Simpl JSON rules engine",
  "private": false,
  "author": "mavarazy@gmail.com",
  "scripts": {
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js --optimize-minimize",
    "dist": "npm run build:lib && npm run build:dist",
    "publish-to-npm": "npm run dist && npm publish && npm version patch",
    "tdd": "jest --watchAll",
    "test": "jest --coverage"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    ".babelrc"
  ],
  "engineStrict": false,
  "engines": {
    "npm": "^2.14.7",
    "node": ">=8"
  },
  "dependencies": {
    "deep-equal": "1.0.1",
    "predicate": "1.2.0",
    "brackets2dots": "^1.1.0",
    "curry2": "^1.0.0",
    "dotsplit.js": "^1.0.3"
  },
  "devDependencies": {
    "atob": "^2.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "codemirror": "^5.29.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "exit-hook": "^1.1.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "gh-pages": "^1.0.0",
    "has-flag": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jsdom": "^11.2.0",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RxNT/json-rules-engine-simplified.git"
  },
  "keywords": [
    "rules",
    "engine",
    "rules engine"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/RxNT/json-rules-engine-simplified#readme"
}
